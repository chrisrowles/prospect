{
  "name": "iceman",
  "version": "0.1.2",
  "description": "A Discord bot and api for Tarkov",
  "main": "index.js",
  "scripts": {
    "postinstall": "tsc",
    "dev": "docker-compose up -d --build proxy",
    "dev:down": "docker-compose down",
    "serve": "node dist/index.js",
    "serve:hot": "nodemon dist/index.js --ignore storage",
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "author": "Chris Rowles <me@rowles.ch>",
  "license": "ISC",
  "dependencies": {
    "commander": "^10.0.0",
    "cors": "^2.8.5",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.2",
    "jsdom": "^21.0.0",
    "mariadb": "^3.1.0",
    "mongodb": "^5.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.29.3",
    "sequelize-typescript": "^2.1.5",
    "typescript": "^4.9"
  },
  "devDependencies": {
    "@types/cors": "^2.8",
    "@types/express": "^4.17",
    "@types/jsdom": "^21.1",
    "@types/mongodb": "^4.0",
    "@types/node": "^18.15.0",
    "@types/swagger-ui-express": "^4.1",
    "@types/validator": "^13.7.13",
    "@typescript-eslint/eslint-plugin": "^5.54",
    "@typescript-eslint/parser": "^5.54",
    "eslint": "^8.35",
    "nodemon": "^2.0",
    "swagger-ui": "^4.18",
    "swagger-ui-express": "^4.6",
    "ts-node": "^10.9"
  }
}
